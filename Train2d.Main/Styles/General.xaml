<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:converter="clr-namespace:Train2d.Main.Converter" 
                    xmlns:system="clr-namespace:System;assembly=mscorlib">
  
  <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter"/>
  <converter:ScaleInverseConverter x:Key="ScaleInverseConverter"/>
  
  <ScaleTransform x:Key="Viewer2DSketchScale" ScaleX="1" ScaleY="-1" CenterX=".5" CenterY=".5" />

  <system:Double x:Key="Viewer2DScaleFactor">1</system:Double>
</ResourceDictionary>